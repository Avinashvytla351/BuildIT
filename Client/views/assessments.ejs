	<style>
		.section-head {
			text-align: center;
			font-family: "Playfair Display", serif;
			color: green;
		}

		.sub-head {
			text-align: center;

			font-family: "Playfair Display", serif;
			color: rgb(0, 0, 0);
			margin-top: 1em;
		}

		.card {
			background-color: #c3c3c6;
			border: 1px solid #dee2e6;
			border-radius: 10px;
			padding: 1em;
			display: flex;
			flex-direction: column;
			justify-content: space-between;
			align-items: center;

		}

		.btn {
			padding: 5px 15px;
			font-size: 14px;
		}

		.section-cards {

			display: flex;
			gap: 4em;
			justify-content: center;
			align-items: center;
			margin-top: 1em;

			a {
				background-color: #41228e;
				padding: 1em;
				border-radius: 0.5em;
				text-align: center;
				color: #fff;
				font-weight: bold;
			}

			a:hover {
				background-color: #2f1a63;
			}

			h3 {
				font-weight: 400;
			}

			svg {
				max-width: 10em;
				max-height: 10em;
			}
		}
	</style>

</head>

