    .team-section {
      display: flex;
      flex-wrap: wrap;
      justify-content: center;
      gap: 1em;
    }

    .team-section-ot {
      display: flex;
      flex-wrap: wrap;
      border-radius: 1em;
      justify-content: center;
      gap: 5%;
    }

    .team-member {
      text-align: center;
      margin: 20px 10px;
      padding: 20px;
      background-color: #f9f9f9e4;
      color: black;
      border-radius: 10px;
      box-shadow: 0 8px 12px rgba(0, 0, 0, 0.51);
      transition: transform 0.3s ease-in-out;
    }

    .team-member-ot {
      text-align: center;
      margin: 20px 10px;
      padding: 20px;
      background-color: #ffffff;
      color: #120534;
      box-shadow: 0 8px 12px rgba(0, 0, 0, 0.51);
      transition: transform 0.3s ease-in-out;
    }

    .team-member:hover {
      transform: translateY(-10px);
    }

    .team-member-ot:hover {
      transform: translateY(-10px);
    }

    .team-member img {
      width: 300px;
      height: 250px;
      object-fit: cover;
      object-position: top;
      margin-bottom: 10px;
    }

    .team-member-ot img {
      width: 300px;
      height: 300px;
      object-fit: cover;
      object-position: top;
      margin-bottom: 10px;
    }

    .team-member h3 {
      font-weight: 700;
      margin: 0;
      color: #000;
      text-align: center;
      font-size: 1.5rem;
    }

    .team-member p {
      color: #000;
    }

    .team-member-ot h3 {
      font-weight: 700;
      margin: 0;
      color: #120534;
      text-align: center;
      font-size: 1.5rem;
    }

    .team-member .role {
      font-size: 1.5rem;
      font-weight: 400;
      color: #000000;
      margin-bottom: 10px;
      margin-top: 10px;
      text-align: center;
    }

    .fa {
      font-size: 24px;
      margin: 0 10px;
      color: #ffffff;
      transition: transform 0.3s ease-in-out, color 0.3s ease-in-out;
      text-decoration: none;
    }

    .team-member:hover .fa {
      transform: scale(1.2);
      color: #007aff;
    }

